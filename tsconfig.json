{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "esModuleInterop": true,
    "strict": true,
    "jsx": "react",
    "rootDir": "./src",
    "baseUrl": "./src",
    "experimentalDecorators": true,
    "noUnusedLocals": true,
    "skipLibCheck": true,
    "lib": [
      "dom",
      "es2015"
    ],
    "paths": {
      "~*": [
        "./*"
      ]
    }
  }
}